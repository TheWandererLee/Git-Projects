<?xml version="1.0" encoding="utf-8"?>
<settings>
    <general_settings>
        <ui_controls>
        	<roll_over>100</roll_over>
        	<roll_out>0</roll_out>
        </ui_controls>
    </general_settings>
    <broadcast>
    	<sldr_volume>
        	<assetTrack>track_btn</assetTrack>
	        <assetThumb>thumb_btn</assetThumb>
	        <display>yes</display>
	        <location>
	            <grid>br</grid>
	            <xOffSet>10</xOffSet>
	            <yOffSet>10</yOffSet>
	        </location>
	        <maximum>60</maximum>
	        <minimum>0</minimum>
	        <orientation>vertical</orientation>
	        <snapInterval>5</snapInterval>
	        <thumbPosition>center</thumbPosition>
	    </sldr_volume>
        <tog_btn_camera>
            <asset>camera_btn</asset>
            <assetSelected>camera_muted_btn</assetSelected>
            <display>yes</display>
            <location>
                <grid>br</grid>
                <xOffSet>75</xOffSet>
                <yOffSet>10</yOffSet>
            </location>
        </tog_btn_camera>
        <tog_btn_microphone>
            <asset>microphone_btn</asset>
            <assetSelected>microphone_muted_btn</assetSelected>
            <display>yes</display>
            <enable>yes</enable>
            <location>
                <grid>br</grid>
                <xOffSet>26</xOffSet>
                <yOffSet>10</yOffSet>
            </location>
        </tog_btn_microphone>
    </broadcast>
    <receive>
        <sldr_volume>
	        <assetTrack>track_btn</assetTrack>
	        <assetThumb>thumb_btn</assetThumb>
	        <display>yes</display>
	        <location>
	            <grid>br</grid>
	            <xOffSet>10</xOffSet>
	            <yOffSet>10</yOffSet>
	        </location>
	        <maximum>200</maximum>
	        <minimum>0</minimum>
	        <orientation>vertical</orientation>
	        <snapInterval>5</snapInterval>
	        <thumbPosition>center</thumbPosition>
	    </sldr_volume>
        <tog_btn_camera>
            <asset>camera_btn</asset>
            <assetSelected>camera_muted_btn</assetSelected>
            <display>yes</display>
            <location>
                <grid>br</grid>
                <xOffSet>75</xOffSet>
                <yOffSet>10</yOffSet>
            </location>
        </tog_btn_camera>
        <tog_btn_speaker>
            <asset>speaker_btn</asset>
            <assetSelected>speaker_muted_btn</assetSelected>
            <display>yes</display>
            <location>
                <grid>br</grid>
                <xOffSet>26</xOffSet>
                <yOffSet>10</yOffSet>
            </location>
        </tog_btn_speaker>
    </receive>
</settings>